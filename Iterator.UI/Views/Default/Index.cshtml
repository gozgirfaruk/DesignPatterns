
@{
    ViewData["Title"] = "Index";
    var model = ViewBag.v;
    int count = 0;
}

<div class="container">
    <br />
    <table class="table table-hover">
        <tr>
            <th>#</th>
            <th>Ziyaret Noktası</th>
        </tr>
        @foreach(var item in model)
        {
            count++;
            <tr>
                <td>@count</td>
                <td>@item</td>
            </tr>
        }
    </table>
</div>
